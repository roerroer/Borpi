<?xml version="1.0" encoding="utf-8"?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5.2"/>
    </startup>
    <appSettings>

      <add key="crono.daysBack" value="6"/>

      <add key="rpiDb" value="Data Source=localhost;Initial Catalog=GPI;Integrated Security=False;User ID=rpi;Password=baktun;MultipleActiveResultSets=True;"/>
    </appSettings>

    <system.diagnostics>
      <trace autoflush="true" indentsize="4">
        <listeners>
          <add name="textWriterListener" type="System.Diagnostics.TextWriterTraceListener" initializeData="DataImport-LogFile.txt"/>
          <remove name="Default"/>
        </listeners>
      </trace>
    </system.diagnostics>
  
</configuration>
