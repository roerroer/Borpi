<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title</title>
    <link href="~/Content/bootstrap.min.css" rel="stylesheet" />
    <link href="~/Content/Gaceta.css" rel="stylesheet" />
    <style>
        .row {
            margin-right: 25px;
            margin-left: 25px;
            border-bottom: 1px solid #ccc;
        }
        .resol {
            margin-right: 45px;
            margin-left: 25px;
        }
    </style>
</head>
<body>
    <div id="hgaceta">
        <div class="row">

            <img style="float:left" src="@Url.Content("~/Logos/RPILogo.jpg")" />

            <div style="float:left; padding-left:15px;">
                <h3> Registro de la Propiedad Intelectual de Guatemala.</h3>
                <h4>@ViewBag.Departamento</h4>
                <h5>Guatemala : @ViewBag._fresol</h5>
                <div class="clearboth"></div>
            </div>

            <div style="float:right">
                @Html.GenerateRelayQrCode(ViewBag.sello as String)
            </div>
        </div>
    </div>
    <div class="resol">
        @RenderBody()
    </div>
</body>
</html>
